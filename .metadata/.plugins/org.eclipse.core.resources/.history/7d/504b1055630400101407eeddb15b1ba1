<script type="text/javascript">
animals={
	dog : "강아지",
	cat : "고양이",
	duck : "오리"
}

const proc1 = () =>{
	// 정보 출력
	str = animals.dog + "<br>"
	str += animals.cat + "<br>"
	str += animals.duck + "<br>"
	document.querySelector('#result1').innerHTML = str;
}

const proc2 = () =>{
	
	/* const {dog, cat, duck} = animals;
	
	str = animals.dog + "<br>"
	str += animals.cat + "<br>"
	str += animals.duck + "<br>"
	 */
	 
	 /* 
	 //일부만
	 const{dog, cat} = animals;

		str = animals.dog + "<br>"
		str += animals.cat + "<br>" */
		
	const{dog, ...rest} = animals;
	console.log(dog,rest)
		
// 	document.querySelector('#result2').innerHTML = str;
}

const proc3=()=>{
	const users= [
		{ name : "철수" , age : 25 },
		{ name : "영희" , age : 30 }
	]
	
	for( const user of users){
		console.log(user.name, user.age);
		
		str = "이름 : " + user.name + "<br>";
		str = "나이 : " + user.age + "<br>";
		
	}
}
</script>
