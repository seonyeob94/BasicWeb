//생성자 함수 -객체 선언
function Rect(garo, sero){
	this.garo = garo;
	this.sero = sero;
	
	this.getArea= function(){
		return this.garo*this.sero;
	}
}

const proc1 = () =>{
	const r1 = new Rect(4, 6);
	const r2 = new Rect(8, 3);
	
	area1 = r1.getArea();
	area2 = r2.getArea();
	
	printRect(r1);
}

