//빈객체 선언. 필요시 동적으로 속성이나 메소드를 추가사용 가능
const rect ={ }

const proc1 = () =>{
	rect.garo =10;
	rect.sero =9;
	
	rect.getArea =function(){
		return this.garo *this.sero;
	}
	
}
