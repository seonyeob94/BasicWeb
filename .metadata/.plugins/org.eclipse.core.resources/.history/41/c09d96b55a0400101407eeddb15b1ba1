<script type="text/javascript">

const myCar = {
		
		model : "x5",
		make : "bmw",
		year : 2020,
		speed : 100,
		
		/* brak : function(){
			this.speed = 0;
		},
		accel : function(){
			this.speed =100;
		} */
		brake() {
			this.speed = 0;
		},
		
		accel() {
			this.speed =100;
		}
}

const proc1 = () => {
	//정보출력
	res= `***myCar의 정보***<br>`;
	res+= `이름 : ${myCar.model}<br>` // ${myCar[model]}
	res+= `제조사 : ${myCar.make}<br>`
	res+= `연식 : ${myCar.year}<br>`

 	document.getElementById('result1').innerHTML = res;
}

const proc2 = () =>{

	res= `===myCar의 정보===<br>`;
	
	str="";
	
	for(let x in myCar){
		//x에는 myCar의 속성 이름이 대입
// 		str += `${x} <br>`;
		str += ` myCar[${x}] : ${myCar[x]} <br> `;
	}
	
	document.getElementById('result2').innerHTML = str;

}

const proc3 = () =>{
	let carKey = Object.keys(myCar);
	console.log(carKey);
	let str = "";
	
// 	for(i=0; i<carKey.length; i++){
// 		str += carKey[i] + "<br>";
// 	}

	for(let key of carKey){
		str += key + "<br>";
	}
	
	carKey.forEach(function(key,index){
		str+= index + "번째 : "+ key +"<br>";
	})
	
	document.getElementById('result3').innerHTML = str;

}

const proc4 = () =>{
	let str = "";
	for(let val of Object.values(myCar)){
		str += val + "<br>";
	}
// 	Object.values(myCar).forEach(function(val, idx){
// 		str += `${idx}번째 : ${val} <br>`;
// 	})
	Object.values(myCar).forEach((val, idx)=>{
		str += `${idx}번째 : ${val} <br>`;
	})
	document.getElementById('result4').innerHTML = str;

}
const proc5 = () =>{
	let entries = Object.entries(myCar);
	console.log(entries);
	
	for(i=0;i<entries.length;i++){
		for(k=0;k<entries[i].length;k++){
			str += entries[i][k]
		}
		str += "<br>"
	}
	document.getElementById('result5').innerHTML = str;
}
</script>
</head>
<body>


<div class="box">
  <h3>객체 정보 출력하기</h3>
  myCar.model / myCar['model']<br><br>
  <input type="button" value="확인" onclick="proc1()">
  <br><br>
  <div id="result1"></div>
</div>


<div class="box">
  <h3>객체 정보 출력하기</h3>
  myCar.model / myCar['model']<br>
  for~ in 반복문을 이용해서 key/value로 접근<br><br>
  <input type="button" value="확인" onclick="proc2()">
  <br><br>
  <div id="result2"></div>
</div>


<div class="box">
  <h3>객체 정보 출력하기</h3>
  myCar.model / myCar['model']<br>
  for~ of 반복문을 이용해서 key/value로 접근<br>
  <span>carKey = Object.keys(myCar);</span> carKey변수는 배열이다<br>
  carValue = Object.value(myCar); carValue변수는 배열이다<br>
  carEntri = Object.entries(myCar); carEntri변수는 배열이다<br><br>
  <input type="button" value="확인" onclick="proc3()">
  <br><br>
  <div id="result3"></div>
</div>

<div class="box">
  <h3>객체 정보 출력하기</h3>
  myCar.model / myCar['model']<br>
  for~ of 반복문을 이용해서 key/value로 접근<br>
  carKey = Object.keys(myCar); carKey변수는 배열이다<br>
  <span>carValue = Object.value(myCar);</span> carValue변수는 배열이다<br>
  carEntri = Object.entries(myCar); carEntri변수는 배열이다<br><br>
  <input type="button" value="확인" onclick="proc4()">
